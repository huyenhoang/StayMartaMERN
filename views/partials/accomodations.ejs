<div class="container">
    <div class="row">
        <div class="Feature_Articles_title">
            <h2>Featured Accommodation</h2>
        </div>
    </div>
    <div class="row">
        <% listings.forEach(function(listings){ %>
        <div class="col-sm-4">
            <div class="thumbnail img-rounded">
                <img class="img-rounded" src="<%= listings.image %>">
                <div class="name-rating">
                    <% for(var i=0; i<listings.rating; i++) {%>
                    <span class="name-rating"><i class="fa fa-star" aria-hidden="true>"></i></span>
                <% } %>
                <br>
                </div>
                <h4 class="listing-name"> <%= listings.name %> </h4>
                <br>
                <p> <%= listings.description %></p>
            </div>
        </div>
        <% }); %>
    </div>
</div>