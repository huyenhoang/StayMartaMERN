<div class="container">
    
    <div class="Feature_Articles_title">
        <h2>Featured Articles</h2>
    </div>
    
    <div class="Feature_Articles">
        <% articles.forEach(function(articles){ %>
            <div class="article">
                <div class="top">
                    <span> <%= articles.title %> </span>
                </div>
                <div class="bottom" style="background: #03B5B5">
                    <p> <%= articles.text %> </p>
                </div>
            </div>
        <% }); %>
    </div>
</div>
