<% include partials/header %>
    <div class='container'>
        <div class="row">
            <h3>Featured Accommodation</h3>
        </div>
        <div class="row">
            <% listings.forEach(function(listings){ %>
            <div class="col-md-3 col-sm-6">
                <div class="thumbnail">
                    <img src="<%= listings.image %>">
                    <h4> <%= listings.name %> </h4>
                </div>
            </div>
            <% }); %>
        </div>
    </div>
<% include partials/footer %>